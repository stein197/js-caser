  "bugs": {
    "url": "https://github.com/stein197/js-caser/issues"
  },
  "homepage": "https://github.com/stein197/js-caser#readme",
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "browserify": "^17.0.0",
    "mocha": "^9.1.3",
    "terser": "^5.9.0",
    "typescript": "^4.4.4"
  }
}
