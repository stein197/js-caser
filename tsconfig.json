{
	"compilerOptions": {
		"target": "ESNext",
		"strict": true,
		"module": "CommonJS",
		"declaration": true,
		"declarationDir": ".",
		"allowSyntheticDefaultImports": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"allowUnusedLabels": false,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"sourceMap": true,
		"inlineSources": true
	},
	"files": [
		"index.ts"
	]
}